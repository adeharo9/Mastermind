<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="125.0" prefWidth="250.0" spacing="30.0" styleClass="background" stylesheets="@../styles/monokai.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.controllers.ExitCurrentGameWarningViewController">
   <children>
      <Label fx:id="warningMessageLabel" layoutX="14.0" layoutY="14.0" maxWidth="220.0" styleClass="text" text="Are you sure you want to exit the current game?" wrapText="true" />
      <HBox alignment="CENTER" layoutX="50.0" layoutY="61.0" spacing="30.0">
         <children>
            <Button fx:id="exitGameButton" mnemonicParsing="false" onAction="#exitGameButtonAction" text="Exit Game" />
            <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancelButtonAction" text="Cancel" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
   </padding>
</VBox>
