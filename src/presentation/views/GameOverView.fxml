<?xml version="1.0" encoding="UTF-8"?>

<?package views?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="300.0" spacing="30.0" styleClass="background" stylesheets="@../styles/monokai.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.controllers.GameOverViewController">
   <children>
      <VBox alignment="CENTER" layoutX="260.0" layoutY="124.0" spacing="15.0">
         <children>
            <Label styleClass="title" text="GAME OVER">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="17.0" prefWidth="79.0">
               <children>
                  <Label styleClass="text" text="SCORE:  " />
                  <Label fx:id="pointsLabel" styleClass="text" />
               </children>
            </HBox>
            <VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="15.0">
               <children>
                <Button mnemonicParsing="false" onAction="#mainMenuButtonAction" prefWidth="123.0" text="Main Menu" textAlignment="CENTER">
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                  </Button>
                  <Button minWidth="-Infinity" mnemonicParsing="false" onAction="#resetGameButtonAction" prefWidth="123.0" text="Reset Game">
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
                  </Button>
                  <Button minWidth="-Infinity" mnemonicParsing="false" onAction="#newGameButtonAction" prefWidth="123.0" text="New Game" />
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="30.0" left="30.0" right="30.0" top="15.0" />
   </padding>
</VBox>
