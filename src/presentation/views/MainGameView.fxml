<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="600.0" prefWidth="400.0" styleClass="background" stylesheets="@../styles/monokai.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.controllers.MainGameViewController">
   <children>
      <ToolBar prefHeight="40.0" prefWidth="402.0" styleClass="toolbar">
         <items>
            <Button fx:id="logOutButton" mnemonicParsing="false" onAction="#logOutButtonAction" text="Log Out" textAlignment="CENTER" />
         </items>
      </ToolBar>
      <VBox layoutX="138.0" layoutY="168.0" prefHeight="223.0" prefWidth="127.0">
         <children>
            <Button fx:id="newGameButton" layoutX="134.0" layoutY="111.0" mnemonicParsing="false" onAction="#newGameButtonAction" prefHeight="25.0" prefWidth="123.0" text="New Game" />
            <Button fx:id="loadGameButton" layoutX="134.0" layoutY="152.0" mnemonicParsing="false" onAction="#loadGameButtonAction" prefHeight="25.0" prefWidth="123.0" text="Load Game" translateY="20.0" />
            <Button fx:id="rankingButton" layoutX="133.0" layoutY="197.0" mnemonicParsing="false" onAction="#rankingButtonAction" prefHeight="25.0" prefWidth="123.0" text="Ranking" translateY="40.0" />
            <Button fx:id="infoButton" layoutX="133.0" layoutY="231.0" mnemonicParsing="false" onAction="#infoButtonAction" prefHeight="25.0" prefWidth="123.0" text="Info" translateY="60.0" />
            <Button fx:id="editUserButton" layoutX="133.0" layoutY="267.0" mnemonicParsing="false" onAction="#editUserButtonAction" prefHeight="25.0" prefWidth="123.0" text="Edit User" translateY="80.0" />
         </children>
      </VBox>
   </children>
</AnchorPane>
