<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox alignment="CENTER" prefHeight="600.0" prefWidth="400.0" spacing="15.0" styleClass="background" stylesheets="@../styles/monokai.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.controllers.LoadingViewController">
   <children>
      <Label styleClass="text" text="Loading ...">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <ProgressIndicator prefHeight="60.0" />
      <Label fx:id="errorLabel" styleClass="error-label" />
   </children>
</VBox>
